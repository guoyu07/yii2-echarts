{
  "name": "yiizh/yii2-echarts",
  "description": "The ECharts extension for Yii2.",
  "homepage": "https://github.com/yiizh/yii2-echarts",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "ECharts"
  ],
  "authors": [
    {
      "name": "Di Zhang",
      "email": "zhangdi_me@163.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*"
  },
  "autoload": {
    "psr-4": {
      "yiizh\\echarts\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}